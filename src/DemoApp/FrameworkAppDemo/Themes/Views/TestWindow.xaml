<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:james="https://jamesnet.dev/xaml/presentation"
    xmlns:ctrls="clr-namespace:XamlDesign.Wpf.UI.Units;assembly=XamlDesign.Wpf"
    xmlns:views="clr-namespace:FrameworkAppDemo.UI.Views">
    <Style TargetType="{x:Type views:TestWindow}"
           BasedOn="{StaticResource {x:Type james:DarkThemeWindow}}">
        <Setter Property="Title" Value="Dicom Manager"/>
        <Setter Property="Content">
            <Setter.Value>
                <james:JamesGrid Rows="Auto, *" Columns="*">
                    <ctrls:IconButton Icon="Facebook" Content="Import"/>
                </james:JamesGrid>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>
